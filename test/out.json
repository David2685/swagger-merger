    }
  },
  "definitions": {
    "$ref#pets": "https://raw.githubusercontent.com/WindomZ/swagger-merger/pets.yaml",
    "errorModel": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}